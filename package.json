{
  "name": "erc721",
  "version": "0.1.0",
  "description": "Reference implementation of the ERC-721 non-fungible token standard.",
  "scripts": {
    "dev": "node ./scripts/dev.js",
    "build": "node ./scripts/build.js",
    "run": "npx hardhat run scripts/xxx.js --network localhost",
    "test": "npx hardhat test",
    "compile": "npx hardhat compile",
    "flatten": "npx hardhat flatten",
    "coverage": "npx hardhat coverage",
    "solhint": "solhint src/**/*.sol",
    "solhint:fix": "solhint --fix src/**/*.sol",
    "doo": "doohickey init -e",
    "lint:js": "doohickey lint:js --ext .js,.jsx,.ts,.tsx ",
    "prepare": "husky install"
  },
  "license": "MIT",
  "repository": {
    "url": "https://github.com/EthanWan/ERC721"
  },
  "homepage": "https://github.com/EthanWan/ERC721#readme",
  "keywords": [
    "ethereum",
    "blockchain",
    "eth",
    "contract",
    "contracts",
    "smart",
    "smart-contract",
    "smart-contracts",
    "token",
    "tokens",
    "nft",
    "nfts",
    "non-fungible",
    "non-fungibles",
    "erc721",
    "erc-721"
  ],
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "2.0.2",
    "@nomiclabs/hardhat-waffle": "2.0.1",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.7",
    "@svgr/webpack": "^6.3.1",
    "@types/node": "^18.7.6",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "babel-loader": "^8.2.5",
    "babel-preset-react-app": "^10.0.1",
    "browserslist": "^4.21.3",
    "chai": "4.3.4",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.0.0",
    "ethereum-waffle": "3.4.0",
    "ethers": "5.5.2",
    "file-loader": "^6.2.0",
    "hardhat": "2.7.0",
    "hardhat-abi-exporter": "2.3.1",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.3",
    "less-loader": "^11.0.0",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-loader": "^7.0.1",
    "postcss-normalize": "^10.0.1",
    "postcss-preset-env": "^7.8.0",
    "react-refresh": "^0.14.0",
    "resolve-url-loader": "^5.0.0",
    "solhint": "3.3.6",
    "solidity-coverage": "0.7.17",
    "source-map-loader": "^4.0.0",
    "style-loader": "^3.3.1",
    "ts-node": "10.4.0",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-dev-server": "^4.10.0",
    "yzm-doohickey": "^0.1.2"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,md,json}": "doohickey lint:prettier --write",
    "**/*.less": "doohickey lint:style --syntax less",
    "**/*.{js,jsx,ts,tsx}": "npm run lint:js"
  },
  "dependencies": {
    "husky": "^8.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
